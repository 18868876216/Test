        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.io.*;

private boolean cmd(String cmd){
	try{
		logBasic(cmd);

		Process p = Runtime.getRuntime().exec(cmd);
		BufferedInputStream in = new BufferedInputStream(p.getInputStream());
        BufferedReader br = new BufferedReader(new InputStreamReader(in));
        String s;
        while ((s = br.readLine()) != null)
          logBasic(s);

	}catch (Exception e){
		logError(cmd, e);
		return false;
   }
	return true;
	
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	boolean rs  = true;
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }
    r = createOutputRow(r, 1000);
	
	///////////////////////begin
	
	String dzhread = get(Fields.In, "dzhread").getString(r);
	String tmppath = get(Fields.In, "tmppath").getString(r);
	String dzhpath = get(Fields.In, "dzhpath").getString(r);
	String param = get(Fields.In, "param").getString(r);
	String sc = getVariable("sc", "SH");
	
	String fn = tmppath + "\\temp" + String.valueOf(String.valueOf(Math.random()).substring(2)) + ".csv";


	String f = param + "\\ETFInfo.ini";

	String t = getVariable("fk", "fkETFComponent");
	String cmd = dzhread + " "+ t + " " + f + " " + fn;// + " 20130705";
	rs = cmd(cmd);

	///////////////////////end
	get(Fields.Out, "fn").setValue(r, fn);
	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return rs;
}

]]></class_source>
