        <class_source><![CDATA[import java.io.*;
import java.net.*;

private boolean download(String url, String f){
		try {
			logBasic("下载数据："+url);
			URL u = new URL(url);
			URLConnection uc = u.openConnection();
			
			OutputStream os = new FileOutputStream(f);
			
			InputStream in = uc.getInputStream();
			BufferedReader r = new BufferedReader(new InputStreamReader(in));
			
			byte[] b = new byte[512];
			int len;
			while ((len=in.read(b)) != -1) {
				os.write(b,0,len);	
			}
			in.close();
			os.close();
			logBasic("下载数据完成："+url);			
			return true;
		} catch (Exception e) {
			logError("下载失败："+url);
			return false;
		}
	}

private boolean cmd(String cmd){
	try{
		logBasic(cmd);

		Process p = Runtime.getRuntime().exec(cmd);
		BufferedInputStream in = new BufferedInputStream(p.getInputStream());
        BufferedReader br = new BufferedReader(new InputStreamReader(in));
        String s;
        while ((s = br.readLine()) != null)
          logBasic(s);

	}catch (Exception e){
		logError(cmd, e);
		return false;
   }
	return true;
	
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	boolean rs  = true;
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }
    r = createOutputRow(r, 1000);
	
	///////////////////////begin
	
	String fn = "\\temp" + String.valueOf(String.valueOf(Math.random()).substring(2));

	String dzhread = get(Fields.In, "dzhread").getString(r);
	String tmppath = get(Fields.In, "tmppath").getString(r);
	String dzhpath = get(Fields.In, "dzhpath").getString(r);
	String sc = getVariable("sc", "SZ");
	

	String f = "\\download\\fin\\full.fin";
	String ft = tmppath + fn;
	String fs = tmppath + fn + ".tmp";
	
	String t = getVariable("fk", "fkSzFundInfo");
	String cmd = "";

	rs = download("http://www.szse.cn/szseWeb/FrontController.szse?ACTIONID=8&CATALOGID=1105&TABKEY=tab1&ENCODE=1",fs);
	if (rs){
		cmd = dzhread + " "+ t + " " + fs + " " + ft + ".csv ";
		rs = cmd(cmd);
	}

	cmd = "cmd /r del " + fs;
	cmd(cmd);
	///////////////////////end
	get(Fields.Out, "fn").setValue(r, ft+".csv");
	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return rs;
}

